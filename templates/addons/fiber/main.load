package mks_modules

import (
    "%%PACKAGE_NAME%%/src/server"
    "%%PACKAGE_NAME%%/src/utils"
)

func loadfiber() {
    server.LoadFiberServer()

	// Setting `WaitGroup` waitgroup routines
	WaitGroup.Add(1)

	// Starting Rest server
	go func() {
		err := server.FiberApplication.Listen(utils.ENV.APP_PORT)
		if err != nil {
			panic(err.Error())
		}
		WaitGroup.Done()
	}()
}